    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Python: Internal Tests - Query",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/source/packages/mojo/testplus/cli/testplus_command.py",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "testing",
                "query",
                "--root=${workspaceFolder}/source/testroots/mojo",
                "--includes=mojo.tests"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Internal Tests - Run",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/source/packages/mojo/testplus/cli/testplus_command.py",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "testing",
                "run",
                "--root=${workspaceFolder}/source/testroots/mojo",
                "--includes=mojo.tests.testplus.local"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Results - Initialize",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/source/packages/mojo/testplus/cli/testplus_command.py",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "publishing",
                "couchdb",
                "initialize",
                "--host",
                "localhost",
                "--username",
                "admin",
                "--password",
                "password"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Results - Publish",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/source/packages/mojo/testplus/cli/testplus_command.py",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "publishing",
                "couchdb",
                "publish",
                "--host",
                "localhost",
                "--username",
                "admin",
                "--password",
                "password",
                "--results",
                "/home/myron/mjr/results/testresults/2023-06-25T2106!48.804103"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Tasker Deploy",
            "type": "python",
            "request": "launch",
